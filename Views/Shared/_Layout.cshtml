<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - crud</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <link rel="stylesheet" href="~/css/site2.css" />
    <script src="https://kit.fontawesome.com/a076d05399.js"></script>
</head>

<body>
    <div class="header">
        <div class="container logo-nav-container">
            <a href="home.html" class="logo">CRUD EXERCISE</a>
        </div>
    </div>
    <header>

        <body>
            <input type="checkbox" id="active">
            <label for="active" class="menu-btn"><i class="fas fa-bars"></i></label>
            <div class="wrapper">
                <ul>
                    <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                    <li><a asp-area="" asp-controller="CiudadanoEstado" asp-action="Index">Ciudadano-Estado</a></li>
                    <li><a asp-area="" asp-controller="ColegioElectoral" asp-action="Index">Colegio Electoral</a></li>
                    <li><a asp-area="" asp-controller="EstadoCivil" asp-action="Index">Estado Civil</a></li>
                    <li><a asp-area="" asp-controller="Municipio" asp-action="Index">Municipio</a></li>
                    <li><a asp-area="" asp-controller="Nacionalidad" asp-action="Index">Nacionalidad</a></li>
                    <li><a asp-area="" asp-controller="Ocupacion" asp-action="Index">Ocupacion</a></li>
                    <li><a asp-area="" asp-controller="Provincia" asp-action="Index">Provincia</a></li>
                    <li><a asp-area="" asp-controller="Sector" asp-action="Index">Sector</a></li>
                    <li><a asp-area="" asp-controller="TipoSangre" asp-action="Index">Tipo Sangre</a></li>
                </ul>
            </div>
        </body>
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await Component.InvokeAsync("Notyf")
    @RenderSection("Scripts", required: false)
</body>

</html>